  "scripts": {
    "pbuild": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "build": "node dist/server.js",
    "start": " ts-node-dev -r tsconfig-paths/register --poll  --inspect --exit-child --respawn --transpile-only --ignore-watch node_modules --no-notify src/server.ts",
    "typeorm": "./node_modules/typeorm/cli.js",
    "migrate": "npm run typeorm migration:generate -- -n",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.5",
    "@babel/node": "^7.15.4",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-typescript": "^7.15.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-standard": "^5.0.0",
    "prettier": "^2.4.1",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-decorators": "^7.17.9",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.0",
    "jest": "^28.0.3",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "ts-jest": "^27.1.4",
    "typeorm": "^0.3.6"
  }
}
